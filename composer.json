{
    "name": "laravel/laravel",
    "type": "project",
    "description": "",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": {
        "gitlab.boxdelivery.com.br/10": {
            "type": "composer",
            "url": "https://gitlab.boxdelivery.com.br/api/v4/group/10/-/packages/composer/packages.json"
        },
        "0": {
            "type": "vcs",
            "url": "https://gitlab.boxdelivery.com.br/box-delivery/squad-core/php-package-messenger.git"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "barryvdh/laravel-cors": "^2.0",
        "core/messenger": "1.0.6",
        "darkaonline/l5-swagger": "^8.0",
        "fideloper/proxy": "^4.3",
        "guzzlehttp/guzzle": "^6.3",
        "hedii/laravel-gelf-logger": "^6.0",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.2",
        "laravel/tinker": "^2.0",
        "ramsey/uuid": "^4.1",
        "redcrystal/cast": "^1.0",
        "spatie/laravel-resource-links": "^1.1"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fakerphp/faker": "^1.15",
        "laravel/dusk": "^6.4",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpmd/phpmd": "@stable",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "3.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "gitlab-domains": ["gitlab.boxdelivery.com.br"]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Core\\": "core/",
            "Database\\": "database/"
        },
        "files": [
            "core/Infrastructure/Helpers/custom-functions.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "box-phpcs": "php vendor\\bin\\phpcs",
        "box-phpcbf": "php vendor\\bin\\phpcbf",
        "box-phpmd": "php vendor\\bin\\phpmd",
        "box-psalm": "php vendor\\bin\\psalm",
        "box-pipeline": "@box-phpcs && @box-phpmd && @box-psalm",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
